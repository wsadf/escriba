{"ast":null,"code":"import { mapGetters } from 'vuex';\nimport { BIcon, BModal } from 'bootstrap-vue';\nexport default {\n  name: 'ModalDetails',\n  components: {\n    BIcon,\n    BModal\n  },\n  props: {\n    id: {\n      type: Number,\n      required: true\n    },\n    showModal: {\n      type: Boolean,\n      required: true\n    }\n  },\n  computed: {\n    ...mapGetters(['detailContact']),\n    detail() {\n      return this.detailContact(this.id);\n    },\n    addressStreet() {\n      const {\n        address,\n        number,\n        complement\n      } = this.detail;\n      if (complement) return `${address}, ${number} - ${complement}`;\n      return `${address}, ${number}`;\n    },\n    openModal: {\n      get() {\n        return this.showModal;\n      },\n      set(value) {\n        this.$emit('change', value);\n      }\n    }\n  }\n};","map":{"version":3,"names":["mapGetters","BIcon","BModal","name","components","props","id","type","Number","required","showModal","Boolean","computed","detail","detailContact","addressStreet","address","number","complement","openModal","get","set","value","$emit"],"sources":["src/components/ModalDetails.vue"],"sourcesContent":["<template>\n  <b-modal v-model=\"openModal\" size=\"lg\">\n    <h2>\n      <b-icon icon=\"person\" variant=\"primary\" />\n      {{ detail.name }}\n    </h2>\n    <p class=\"mb-1\">{{ detail.phone }}</p>\n    <p>{{ detail.email }}</p>\n    <p class=\"pt-4 h5\">Dados</p>\n    <hr class=\"my-1\" />\n    <p class=\"mb-1\">{{ addressStreet }}</p>\n    <p class=\"mb-1\">{{ detail.neighborhood }} - {{ detail.city }}/{{ detail.state }}</p>\n    <p>CEP: {{ detail.zipCode }}</p>\n    <p>CPF: {{ detail.cpf }}</p>\n    <p>Data de nascimento: {{ detail.birth }}</p>\n\n    <template #modal-footer>\n      <b-button\n        variant=\"primary\"\n        @click=\"openModal = false\"\n      >\n        Fechar\n      </b-button>\n    </template>\n  </b-modal>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex';\nimport { BIcon, BModal } from 'bootstrap-vue'\n\nexport default {\n  name: 'ModalDetails',\n\n  components: {\n    BIcon,\n    BModal\n  },\n\n  props: {\n    id: {\n      type: Number,\n      required: true,\n    },\n    showModal: {\n      type: Boolean,\n      required: true,\n    }\n  },\n\n  computed: {\n    ...mapGetters(['detailContact']),\n\n    detail() {\n      return this.detailContact(this.id);\n    },\n\n    addressStreet() {\n      const { address, number, complement } = this.detail;\n      if (complement) return `${address}, ${number} - ${complement}`;\n      return `${address}, ${number}`\n    },\n\n    openModal: {\n      get() {\n        return this.showModal;\n      },\n      set(value) {\n        this.$emit('change', value);\n      },\n    },\n  },\n}\n</script>\n"],"mappings":"AA4BA,SAAAA,UAAA;AACA,SAAAC,KAAA,EAAAC,MAAA;AAEA;EACAC,IAAA;EAEAC,UAAA;IACAH,KAAA;IACAC;EACA;EAEAG,KAAA;IACAC,EAAA;MACAC,IAAA,EAAAC,MAAA;MACAC,QAAA;IACA;IACAC,SAAA;MACAH,IAAA,EAAAI,OAAA;MACAF,QAAA;IACA;EACA;EAEAG,QAAA;IACA,GAAAZ,UAAA;IAEAa,OAAA;MACA,YAAAC,aAAA,MAAAR,EAAA;IACA;IAEAS,cAAA;MACA;QAAAC,OAAA;QAAAC,MAAA;QAAAC;MAAA,SAAAL,MAAA;MACA,IAAAK,UAAA,YAAAF,OAAA,KAAAC,MAAA,MAAAC,UAAA;MACA,UAAAF,OAAA,KAAAC,MAAA;IACA;IAEAE,SAAA;MACAC,IAAA;QACA,YAAAV,SAAA;MACA;MACAW,IAAAC,KAAA;QACA,KAAAC,KAAA,WAAAD,KAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}